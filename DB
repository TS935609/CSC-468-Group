CREATE DATABASE CoinMiner;
SHOW DATABASES;
USE CoinMiner;
CREATE TABLE Credentials
(
	email varchar(100),
    password varchar(25),
    FirstName varchar(30),
    LastName varchar(30)
);
INSERT INTO Credentials (email,password,FirstName,LastName)VALUES("olivia123@gmail.com","Oliviac","Olivia","georgiana");
INSERT INTO Credentials (email,password,FirstName,LastName) VALUES("jtkelly@hotmail.com","jtkelly","John","Kelly");
USE CoinMiner;
CREATE TABLE MyPortfolio
(
	CoinType varchar(20),
    Quantity int(255)
);
INSERT INTO MyPortfolio (CoinType,Quantity) Values("Bitcoin", 5);
INSERT INTO MyPortfolio (CoinType,Quantity) Values("Dogecoin", 5);
INSERT INTO MyPortfolio (CoinType,Quantity) Values("Ethereum", 5);
USE CoinMiner;
CREATE TABLE CoinMinerBoard
(
	CoinType varchar(25),
    Price decimal(65,2),
    Quantity int(100)
);
INSERT INTO CoinMinerBoard (CoinType,Price,Quantity) VALUES("Bitcoin",43976.09,20);
INSERT INTO CoinMinerBoard (CoinType,Price,Quantity) Values("Dogecoin",0.11,250);
INSERT INTO CoinMinerBoard (CoinType,Price,Quantity) Values("Ethereum",3502.23,50);
SHOW TABLES;
SELECT*FROM CoinMinerBoard;
SELECT*FROM Credentials;
SELECT*FROM MyPortfolio;
